{
    "name": "alright-components",
    "version": "0.0.2",
    "main": "./lib/index.js",
    "engines": {
        "node": ">=4.0.0"
    },
    "author": {
        "name": "Seth Davis",
        "email": "techwithseth512@gmail.com"
    },
    "files": [
        "lib"
    ],
    "keywords": [
        "alright",
        "components",
        "react"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/setholito/alright-components"
    },
    "homepage": "https://sethdavis512.github.io/alright-components/",
    "license": "(MIT OR Apache-2.0)",
    "dependencies": {
        "highlight.js": "^9.13.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.1.0",
        "@babel/plugin-transform-modules-commonjs": "^7.4.3",
        "@svgr/webpack": "2.4.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "9.0.0",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.4",
        "babel-plugin-named-asset-import": "^0.2.3",
        "babel-preset-react-app": "^6.1.0",
        "bfj": "6.1.1",
        "case-sensitive-paths-webpack-plugin": "2.1.2",
        "chalk": "2.4.1",
        "chokidar": "^2.0.4",
        "cpx": "^1.5.0",
        "cross-env": "^4.0.0",
        "css-loader": "1.0.0",
        "dotenv": "6.0.0",
        "dotenv-expand": "4.2.0",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.7.1",
        "enzyme-to-json": "^3.3.5",
        "eslint": "5.6.0",
        "eslint-config-react-app": "^3.0.5",
        "eslint-loader": "2.1.1",
        "eslint-plugin-flowtype": "2.50.1",
        "eslint-plugin-import": "2.14.0",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-react": "7.11.1",
        "file-loader": "2.0.0",
        "fork-ts-checker-webpack-plugin-alt": "0.4.14",
        "fs-extra": "7.0.0",
        "gh-pages": "^2.0.1",
        "html-webpack-plugin": "4.0.0-alpha.2",
        "identity-obj-proxy": "3.0.0",
        "jest": "23.6.0",
        "jest-pnp-resolver": "1.0.1",
        "jest-resolve": "23.6.0",
        "mini-css-extract-plugin": "0.4.3",
        "npm-run-all": "^4.1.5",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "pnp-webpack-plugin": "1.1.0",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-preset-env": "6.0.6",
        "postcss-safe-parser": "4.0.1",
        "prop-types": "^15.6.2",
        "react-addons-test-utils": "^15.6.2",
        "react-app-polyfill": "^0.1.3",
        "react-dev-utils": "^6.1.1",
        "react-docgen": "^2.21.0",
        "react-test-renderer": "^16.6.3",
        "resolve": "1.8.1",
        "rimraf": "^2.6.1",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.0",
        "terser-webpack-plugin": "1.1.0",
        "url-loader": "1.1.1",
        "webpack": "4.19.1",
        "webpack-dev-server": "^3.3.0",
        "webpack-manifest-plugin": "2.0.4",
        "workbox-webpack-plugin": "3.6.3"
    },
    "scripts": {
        "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore test.js",
        "build:css": "cpx \"./src/components/**/*.css\" ./lib",
        "build:docs": "node scripts/build.js",
        "build:lib": "npm run build:commonjs && npm run build:css",
        "deploy:docs": "gh-pages -d build",
        "gen:docs:watch": "npm run gen:docs -- --watch",
        "gen:docs": "node scripts/generateComponentData.js",
        "prebuild:lib": "rimraf lib",
        "predeploy:docs": "npm run build:docs",
        "prestart": "npm run gen:docs",
        "start:docs": "node scripts/start.js",
        "start": "npm-run-all --parallel start:docs gen:docs:watch",
        "test": "node scripts/test.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "resolver": "jest-pnp-resolver",
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupTestFrameworkScriptFile": "./setupJest.js",
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "babel": {
        "plugins": [
            [
                "@babel/plugin-transform-modules-commonjs",
                {
                    "modules": "commonjs"
                }
            ]
        ],
        "presets": [
            "react-app"
        ]
    }
}
